<Window x:Class="DialingVpnEntryWithWPF.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Create and Dial VPN Entry Example" Height="381" Width="595" Initialized="Window_Initialized">
    <Grid>
        <TextBox Margin="12,12,12,75.371" Name="StatusTextBox" VerticalScrollBarVisibility="Visible" TextWrapping="WrapWithOverflow" />
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,12,12" Name="DialButton" VerticalAlignment="Bottom" Width="75" Click="DialButton_Click" IsEnabled="False">Dial</Button>
        <ComboBox Height="22.684" Margin="12,0,254.133,12.316" Name="ConnectionComboBox" VerticalAlignment="Bottom" SelectionChanged="ConnectionComboBox_SelectionChanged" />
        <Label Height="28" HorizontalAlignment="Left" Margin="12,0,0,41.354" Name="ConnectionLabel" VerticalAlignment="Bottom" Width="120">Connection Name:
        </Label>
        <Button Content="Disconnect" Height="23" HorizontalAlignment="Right" Margin="0,0,93,12" Name="DisconnectButton" VerticalAlignment="Bottom" Width="75" IsEnabled="False" Click="DisconnectButton_Click" />
    </Grid>
</Window>
